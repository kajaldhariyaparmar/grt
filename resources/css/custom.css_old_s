/*=========== BEGIN NEWS SECTION ============*/

.home-news-section {
    border-bottom: 1px solid #bbbbbb;
}

.home-news-section .section-title {
    padding-bottom: 15px;
}

.home-news-section .section-title h2 {
    text-align: left;
}

.home-news-content {
    float: left;
    width: 100%;
}

.home-news-content ul {
    float: left;
    width: 100%;
    margin: 0px;
    padding: 0px;
    list-style: none;
}

.home-news-content ul li {
    float: left;
    width: 100%;
    margin: 0px;
    padding: 25px 0px;
    border-bottom: 1px solid #f2f2f2;
}

.home-news-item-wrap {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: flex-start;
    flex-wrap: wrap;
    list-style-type: none;
}

.home-news-item-wrap .column {
    position: relative;
    flex: 0 1 33.333333%;
    display: flex;
    justify-content: space-between;
    align-self: stretch;
    margin: 0px;
    padding: 0px;
    border: 0px;
    border-radius: 0px;
    box-shadow: none;
    outline: none;
    transition: box-shadow 0.2s ease-in-out;
    flex-direction: row;
    align-items: center;
}

.home-news-item-wrap .column.news-date-col {
    flex: 0 1 25%;
}

.home-news-item-wrap .column.news-title-col {
    flex: 0 1 50%;
}

.home-news-item-wrap .column.news-read-more-col {
    flex: 0 1 25%;
    justify-content: flex-end;
}

.home-news-item-wrap .news-date {
    font-size: 18px;
    line-height: 120%;
}

.home-news-item-wrap .news-title {
    font-size: 18px;
    line-height: 120%;
    margin: 0px;
    font-weight: 300;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.home-news-item-wrap .news-read-more {
    font-size: 18px;
    line-height: 120%;
    margin: 0px;
    font-weight: 300;
    color: #009FDF;
    text-decoration: underline;
}

.home-news-item-wrap .news-read-more:hover {
    text-decoration: none;
}


/*=========== END NEWS SECTION ============*/


/*=========== BEGIN HADITH FEATURED SECTION ============*/

.hadith-featured-section {
    padding-bottom: 700px;
}

.hadith-content {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-wrap: wrap;
    max-width: 1000px;
    margin: 0 auto;
    text-align: center;
    padding-bottom: 50px;
}

.hadith-content h3 {
    font-size: 30px;
    line-height: 120%;
    font-weight: 300;
    padding-bottom: 20px;
}

.featured-content {
    float: left;
    width: 100%;
}

.featured-grid {
    display: grid;
    align-items: center;
    justify-content: center;
    width: 100%;
    margin: 0 auto;
    -ms-grid-columns: (1fr)[4];
    grid-template-columns: repeat(4, 1fr);
    column-gap: 20px;
    row-gap: 20px;
    list-style: none;
}

.featured-grid li {
    float: left;
    width: 100%;
    margin: 0px;
    padding: 0px;
    cursor: pointer;
}

.featured-item {
    float: left;
    width: 100%;
    background: #ffffff;
    box-shadow: 5px 5px 25px rgba(0, 0, 0, 0.3);
    border-radius: 10px;
    padding: 40px;
}

.featured-item .icon-wrap {
    float: left;
    width: 100%;
    overflow: hidden;
    margin-bottom: 20px;
    text-align: center;
}

.featured-item .icon-wrap img {
    float: none;
    object-fit: cover;
    max-width: 125px;
    margin: 0 auto;
}

.featured-item .featurd-title {
    font-size: 20px;
    line-height: 120%;
    font-weight: 600;
    text-align: center;
    margin-bottom: 5px;
}

.expandable-wrap {
    float: left;
    width: 100%;
    text-align: center;
    overflow: hidden;
    max-height: 0px;
    transition: all 0.8s ease;
}

.expandable-wrap p {
    font-size: 14px;
    line-height: 120%;
    font-weight: 300;
    color: #777777;
    text-align: center;
    padding: 0px 0px 10px;
}

.expandable-wrap a {
    font-size: 16px;
    line-height: 120%;
    font-weight: 600;
    color: #009FDF;
    text-align: center;
    text-decoration: underline;
}

.featured-grid li.active .expandable-wrap {
    max-height: 500px;
    -webkit-transition: max-height 0.35s ease-in;
    -moz-transition: max-height 0.35s ease-in;
    -o-transition: max-height 0.35s ease-in;
    -ms-transition: max-height 0.35s ease-in;
    transition: max-height 0.35s ease-in;
}


