/*=========== BEGIN COMMON CSS ============*/

body {
    font-family: 'Inter', sans-serif;
    font-size: 16px;
    line-height: 120%;
    font-weight: 400;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    color: #111111;
}

.txt-color-black {
    color: #111111;
}

.txt-color-grey {
    color: #777777;
}

.txt-color-light-grey {
    color: #F2F2F2;
}

.txt-color-white {
    color: #ffffff;
}

.txt-color-light-blue {
    color: #1D72E7;
}

.txt-color-blue {
    color: #174EA6;
}

.txt-color-red {
    color: #E10032;
}

.bg-grey {
    background: #777777;
}

.bg-light-grey {
    background: #F2F2F2;
}

.bg-white {
    background: #ffffff;
}

.bg-light-blue {
    background: #1D72E7;
}

.bg-blue {
    background: #174EA6;
}

.bg-red {
    background: #E10032;
}

.bg-deep-grey {
    background: #4A4A4A;
}

.bg-light-black {
    background: #111111;
}

.section-title h2 {
    font-weight: 300;
}

.three-col-grid {
    display: grid;
    align-items: center;
    justify-content: center;
    width: 100%;
    margin: 0 auto;
    -ms-grid-columns: (1fr)[3];
    grid-template-columns: repeat(3, 1fr);
    column-gap: 20px;
    row-gap: 20px;
    padding: 0 15px!important;
}

.single-header-footer {
    background: #ffffff;
}

.single-header-footer .main-wrap-inner {
    padding-bottom: 60px;
    background: #ffffff;
}


/*=========== END COMMON CSS ============*/


/*=========== BEGIN HEADER ============*/

.header-fundraise-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    width: auto;
    margin-right: 10px;
}

.header-fundraise-btn .material-icons {
    font-size: 18px;
    margin-right: 5px;
}

.header-fundraise-btn .btn-wrap,
.header-donate-btn .btn-wrap {
    min-width: 160px;
    display: flex;
    align-items: center;
    justify-content: center;
}

.header-donate-btn .cart-icon-wrap {
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}

.header-cart-count {
    /*    position: absolute;
    font-size: 12px;
    line-height: 18px;
    font-weight: 600;
    top: -12px;*/
    margin-left: 5px;
}

.header-donate-btn .material-icons-outlined {
    font-size: 18px;
    margin-right: 5px;
}

.nav-container ul li a {
    line-height: 64px;
}

li.small-submenu {
    position: relative!important;
}

li.small-submenu .dropdown-menu-wrap {
    position: absolute;
    top: 142px;
    left: 0px;
    display: block;
    overflow: hidden;
    max-height: 0px;
    -webkit-transition: max-height 0.3s ease-in;
    -moz-transition: max-height 0.3s ease-in;
    -o-transition: max-height 0.3s ease-in;
    -ms-transition: max-height 0.3s ease-in;
    transition: max-height 0.3s ease-in;
    z-index: 99999;
}

.dropdown-menu-wrap,
.dropdown-menu-content {
    float: left;
    width: 100%;
}

ul.main-menu li.small-submenu.active .dropdown-menu-wrap {
    max-height: 500px;
}

.dropdown-menu-ul-wrap {
    float: left;
    width: 100%;
}

.dropdown-menu-ul {
    float: left;
    width: 100%;
    list-style: none;
    margin: 0px;
    padding: 0px;
}

.dropdown-menu-ul>li {
    display: block;
    margin: 0px!important;
    padding: 0px!important;
    list-style: none;
    width: 100%;
    min-height: 10px!important;
}

.dropdown-menu-ul li>a {
    display: block;
    width: 100%;
    background: #009FDF;
    font-size: 16px;
    line-height: 120%!important;
    color: #ffffff!important;
    padding: 15px 20px 13px!important;
    margin: 0px!important;
    border-bottom: 1px solid rgba(255, 255, 255, 0.25)!important;
}

.dropdown-menu-ul li>a:hover,
.dropdown-menu-ul li>a.active {
    background: #31b8ee;
    border-bottom: 1px solid rgba(255, 255, 255, 0.25)!important;
}

.dropdown-menu-ul li>a.active::after {
    width: 0px!important;
}

.dropdown-menu-ul li:last-child a {
    border-bottom: transparent!important;
}

li.small-submenu .dropdown-menu-wrap {
    max-width: 240px;
    min-width: 240px;
    top: 64px;
    left: 0px;
}

span.dropdown-arrow {
    width: 30px;
    right: 0px;
    top: 0px;
    text-align: center;
    font-size: 14px;
    line-height: 1;
    color: #009FDF;
    cursor: pointer;
    -webkit-transform: rotate(90deg);
    -moz-transform: rotate(90deg);
    -o-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    transform: rotate(90deg);
}

li.small-submenu.active>a,
li.small-submenu.active>span.dropdown-arrow {
    color: #31b8ee;
}

.single-header .header-top-wrap {
    background: #FFFFFF;
}

.single-header-close {
    float: left;
    width: 100%;
    text-align: right;
}

li.small-submenu:hover .dropdown-menu-wrap {
    max-height: 700px!important;
}


/*=========== END HEADER ============*/


/*=========== BEGIN HOME HERO SECTION ============*/

#homeSlider .flex-direction-nav a:before {
    font-family: 'Font Awesome 5';
    font-size: 38px;
    line-height: 1;
    opacity: 1;
    color: #ffffff;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    position: relative;
    top: 1px;
    font-weight: 900;
}

#homeSlider .flex-direction-nav a.flex-prev {
    left: 20px;
    opacity: 1;
}

#homeSlider .flex-direction-nav a.flex-next {
    right: 20px;
    opacity: 1;
}

#homeSlider .flex-direction-nav a.flex-prev:before {
    content: "\f053";
}

#homeSlider .flex-direction-nav a.flex-next:before {
    content: "\f054";
}

.banner-caption-wrap .container {
    max-width: 90%;
}

.banner-caption-content {
    float: left;
    background: transparent!important;
}

.banner-caption-wrap h3 {
    padding: 0px;
    color: #ffffff;
    text-transform: uppercase;
}

.banner-caption-wrap h1 {
    font-size: 50px;
    font-weight: 600;
    padding: 0px;
    text-transform: uppercase;
    color: #ffffff;
}

.banner-caption-wrap h1 span {
    color: #009FDF;
    text-shadow: 0 0 7px #fff, 0 0 10px #fff, 0 0 21px #fff, 0 0 42px #009FDF, 0 0 82px #009FDF, 0 0 92px #009FDF, 0 0 102px #009FDF, 0 0 151px #009FDF;
}

.banner-caption-content .custom-row .btn-wrap {
    margin-top: 20px;
}

.slider-direction-nav {
    position: absolute;
    width: 100%;
    top: 50%;
    z-index: 1;
    height: 1px;
}

.slider-direction-nav ul {
    float: left;
    width: 100%;
    margin: 0px;
    padding: 0px;
    list-style: none;
}

.slider-direction-nav ul li {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    justify-content: center;
    float: left;
    width: 40px;
    height: 40px;
    margin: 0px;
    padding: 0px;
    list-style: none;
    position: absolute;
    left: 20px;
    top: -20px;
    color: #ffffff;
    cursor: pointer;
}

.slider-direction-nav ul li:hover {
    color: #009FDF;
}

.slider-direction-nav ul li.slide-next {
    left: auto;
    right: 20px;
}

.slider-direction-nav ul li span {
    font-size: 40px;
}


/*=========== END HOME HERO SECTION ============*/


/*=========== BEGIN ABOUT SECTION ============*/

